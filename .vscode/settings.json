{
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "json.schemas": [
    {
      "fileMatch": [
        "tsconfig.*.json"
      ],
      "url": "http://json.schemastore.org/tsconfig"
    }
  ],
  "markdownlint.ignore": [
    "LICENSE"
  ],
  "eslint.enable": true,
  "cSpell.words": [
    "jsnext",
    "libm",
    "Luka",
    "Miku",
    "pinst",
    "sateisfier",
    "snyk",
    "tersified",
    "tersify",
    "tsbuildinfo"
  ],
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "eslint.nodePath": ".yarn/sdks",
  "typescript.enablePromptUseWorkspaceTsdk": true
}
